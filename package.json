{
  "name": "audio-ecommerce-monorepo",
  "private": true,
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && mvnw spring-boot:run -Dspring-boot.run.arguments=--spring.config.import=optional:file:.env",
    "start:all": "concurrently -n \"FRONT,BACK\" -c \"cyan,green\" \"npm run dev:frontend\" \"npm run dev:backend\""
  },
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
